<launch>

    <arg name="project" default="lio_sam"/>

    <!--- Sensor Fusion -->
    <!-- <node pkg="rtk_imu" type="multiSensorFusion" name="fusion" output="screen"/> -->

    <!-- Parameters -->
    <rosparam file="$(find lio_sam)/config/params_lgsvl.yaml" command="load" />

    <!--- LOAM -->
    <include file="$(find lio_sam)/launch/include/module_loam.launch" />

    <!--- Robot State TF -->
